{
  "extends":
  [
    "standard"
  ],
  "rules":
  {
    "import/newline-after-import":
    [
      "error",
      {
        "count": 1
      }
    ],
    "n/no-callback-literal": "off",
    "semi":
    [
      "error",
      "always"
    ],
    "comma-dangle":
    [
      "error",
      "always-multiline"
    ],
    "space-before-function-paren":
    [
      "error",
      "never"
    ],
    "no-unused-vars":
    [
      "error",
      {
        "args": "after-used"
      }
    ]
  }
}